{
  "name": "iii-sdk",
  "version": "0.3.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "author": "III",
  "license": "Apache-2.0",
  "type": "module",
  "description": "III SDK for Node.js",
  "keywords": ["iii", "sdk"],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./streams": {
      "types": "./dist/streams.d.ts",
      "import": "./dist/streams.mjs",
      "require": "./dist/streams.cjs"
    },
    "./telemetry": {
      "types": "./dist/telemetry.d.ts",
      "import": "./dist/telemetry.mjs",
      "require": "./dist/telemetry.cjs"
    }
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.57.0",
    "@opentelemetry/core": "^1.30.0",
    "@opentelemetry/instrumentation": "^0.57.0",
    "@opentelemetry/otlp-transformer": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-logs": "^0.57.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.0"
  }
}